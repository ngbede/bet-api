{
  "name": "bet-api",
  "version": "1.0.0",
  "description": "POC for automatic betting code translation between multiple betting sites.",
  "main": "app.js",
  "author": "ngbede",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc -w\" \"nodemon -q build/app.js\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node build/app.js"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "morgan": "^1.10.0",
    "puppeteer": "^17.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.31",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2"
  }
}
